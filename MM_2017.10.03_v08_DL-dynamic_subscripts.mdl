{UTF-8}
"% Missed Appt Rate"[Diagnoses]=
	GET DIRECT CONSTANTS('ModelParameters.xlsx','CCParams','B3')
	~	dmnl
	~		|

"% of Patients with AUD, DEP, OUD, and OtherNeeds"[Diagnoses]=
	0.5, 0.5, 0.5, 0.5
	~	dmnl
	~	The proportion of a team's patients who are diagnosed with AUD, DEP, and \
		OUD.  These are not exclusive catagories, so they should sum to >1.
	|

"% One and Done Appt Rate"[Diagnoses]=
	GET DIRECT CONSTANTS('ModelParameters.xlsx','CCParams','B2')
	~	dmnl
	~		|

Avg MM Care Episode[Diagnoses]=
	GET DIRECT CONSTANTS('ModelParameters.xlsx','CCParams','B12')
	~	Week
	~		|

Desired RVI[Diagnoses]=
	12,12,4, 12
	~	Week
	~		|

Diagnoses:
	AUD, DEP, OUD, OtherNeeds
	~	Patients
	~	The three key diagnoses.
	|

Effect of Team Perception of MM Availability on Scheduling New Patients[Diagnoses]= WITH LOOKUP\
		 (
	Team Perception of MM Availability[Diagnoses],
		([(0,0)-(1,2)],(0,1),(1,2) ))
	~	dmnl
	~		|

Effect of WP on Attendance Rate for AUD and DEP only[Diagnoses]= WITH LOOKUP (
	Work Pressure[Diagnoses],
		([(0,1)-(1,10)],(0,1),(0.0458716,6.05263),(0.0948012,7.98246),(0.134557,8.90351),(0.262997\
		,9.25439),(0.33945,9.42982),(0.547401,9.60526),(1,10) ))
	~	dmnl
	~		|

Effect of WP on Missed Appt Rate[Diagnoses]= WITH LOOKUP (
	Work Pressure[Diagnoses],
		([(0,1)-(1,10)],(0,10),(0.24159,9.32895),(0.510703,8.26316),(0.801223,4.86842),(1,1\
		) ))
	~	dmnl
	~		|

Expected New MM Patient Rate[Diagnoses]=
	GET DIRECT CONSTANTS('ModelParameters.xlsx','CCParams','B1')
	~	Patients/Week
	~		|

"Ideal % of supply allocated to AUD, DEP, OUD, and OtherNeeds"[Diagnoses]=
	((MM Supply Data*"% of Patients with AUD, DEP, OUD, and OtherNeeds"[Diagnoses])/Desired RVI\
		[Diagnoses])*Avg MM Care Episode[Diagnoses]
	~	Appts
	~		|

"Maximum Possible Scheduling Rate - MM"[Diagnoses]=
	MM Patients Waiting for 1st Visit[Diagnoses]/"Minimum Scheduling Delay - MM"[Diagnoses\
		]
	~	Patients/Week
	~		|

"Minimum Scheduling Delay - MM"[Diagnoses]=
	1
	~	Week
	~		|

MM Appts Attended[Diagnoses]= INTEG (
	MM Attendance Rate[Diagnoses],
		0)
	~	Appts
	~		|

MM Appts Scheduled[Diagnoses]= INTEG (
	+MM Scheduling Rate[Diagnoses]-MM Attendance Rate[Diagnoses]-MM Missed Appt Rate[Diagnoses\
		]+MM Resheduling Rate[Diagnoses],
		1)
	~	Appts
	~		|

MM Attendance Rate[Diagnoses]=
	(MM Appts Scheduled[Diagnoses]*(1-"% Missed Appt Rate"[Diagnoses]))/MM Return Interval\
		[Diagnoses]
	~	Appts/Week
	~		|

MM Becoming a Patient[Diagnoses]=
	MIN("Maximum Possible Scheduling Rate - MM"[Diagnoses],Patients Receiving 1st MM Appt\
		[Diagnoses])
	~	Patients/Week
	~		|

MM Existing Patient Visit Frequency[Diagnoses]=
	MM Attendance Rate[Diagnoses]*"% One and Done Appt Rate"[Diagnoses]
	~	Appts/Week
	~		|

MM Missed Appt Rate[Diagnoses]=
	(MM Appts Scheduled[Diagnoses]*"% Missed Appt Rate"[Diagnoses])/(MM Return Interval[\
		Diagnoses]/Effect of WP on Missed Appt Rate[Diagnoses])
	~	Appts/Week
	~		|

MM New Patient Visit Frequency[Diagnoses]=
	MM Becoming a Patient[Diagnoses]/Pts per Appt[Diagnoses]
	~	Appts/Week
	~		|

"MM No-Shows to Reschedule"[Diagnoses]= INTEG (
	MM Missed Appt Rate[Diagnoses]-MM Resheduling Rate[Diagnoses],
		0)
	~	Appts
	~		|

MM One and Done Patients[Diagnoses]= INTEG (
	+MM One and Done Stopping[Diagnoses],
		0)
	~	Patients
	~		|

MM One and Done Stopping[Diagnoses]=
	(MM Patients[Diagnoses]*"% One and Done Appt Rate"[Diagnoses])/MM time to first appt\
		[Diagnoses]
	~	Patients/Week
	~		|

MM Patients[Diagnoses]= INTEG (
	+MM Becoming a Patient[Diagnoses]-MM Stopping[Diagnoses]-MM One and Done Stopping[Diagnoses\
		],
		0)
	~	Patients
	~		|

MM Patients Waiting for 1st Visit[Diagnoses]= INTEG (
	+MM Scheduling New Patients[Diagnoses]+MM Becoming a Patient[Diagnoses],
		1)
	~	Patients
	~		|

MM Resheduling Rate[Diagnoses]=
	"MM No-Shows to Reschedule"[Diagnoses]/"Minimum Scheduling Delay - MM"[Diagnoses]
	~	Appts/Week
	~		|

MM Return Interval[OUD]=
	Desired RVI[OUD] ~~|
MM Return Interval[DEP]=
	Desired RVI[DEP]/Effect of WP on Attendance Rate for AUD and DEP only[DEP] ~~|
MM Return Interval[AUD]=
	Desired RVI[AUD]/Effect of WP on Attendance Rate for AUD and DEP only[AUD] ~~|
MM Return Interval[OtherNeeds]=
	Desired RVI[OtherNeeds]/Effect of WP on Attendance Rate for AUD and DEP only[OtherNeeds\
		]
	~	Week
	~		|

MM Scheduling Efficiency[Diagnoses]=
	1
	~	dmnl
	~	The percent of open slots actually used for new patients.
	|

MM Scheduling New Patients[Diagnoses]=
	Effect of Team Perception of MM Availability on Scheduling New Patients[Diagnoses]*Expected New MM Patient Rate\
		[Diagnoses]
	~	Patients/Week
	~		|

MM Scheduling Rate[Diagnoses]=
	IF THEN ELSE(MM Appts Scheduled[Diagnoses]<=MM Total Supply[Diagnoses], (MM New Patient Visit Frequency\
		[Diagnoses]+MM Existing Patient Visit Frequency[Diagnoses]) 
	, 0 )
	~	Appts/Week
	~		|

MM Stopping[Diagnoses]=
	(MM Patients[Diagnoses]*(1-"% One and Done Appt Rate"[Diagnoses]))/Avg MM Care Episode\
		[Diagnoses]
	~	Patients/Week
	~		|

MM Supply Data=
	100
	~	Appts
	~		|

MM Supply for New Patients[Diagnoses]=
	MAX((MM Total Supply[Diagnoses]-MM Supply Used by All Pts[Diagnoses]),0)
	~	Appts
	~		|

MM Supply Used by All Pts[Diagnoses]=
	MM Appts Scheduled[Diagnoses]+"MM No-Shows to Reschedule"[Diagnoses]
	~	Appts
	~		|

MM time to first appt[Diagnoses]=
	GET DIRECT CONSTANTS('ModelParameters.xlsx','CCParams','B7')
	~	Week
	~		|

MM Total Supply[Diagnoses]=
	"Ideal % of supply allocated to AUD, DEP, OUD, and OtherNeeds"[Diagnoses]
	~	Appts
	~		|

Patients Receiving 1st MM Appt[Diagnoses]=
	(MM Scheduling Efficiency[Diagnoses]*MM Supply for New Patients[Diagnoses]*Pts per Appt\
		[Diagnoses])/"Minimum Scheduling Delay - MM"[Diagnoses]
	~	Patients/Week
	~		|

Pts per Appt[Diagnoses]=
	1
	~	Patients/Appts
	~		|

Team Perception of MM Availability[Diagnoses]=
	MM Patients[Diagnoses]/MM Patients Waiting for 1st Visit[Diagnoses]
	~	dmnl
	~		|

Work Pressure[Diagnoses]=
	MM Appts Scheduled[Diagnoses]/MM Total Supply[Diagnoses]
	~	dmnl
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 260
	~	Week
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	Week
	~	The initial time for the simulation.
	|

SAVEPER  = 1
	~	Week [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 0.03125
	~	Week [0,?]
	~	The time step for the simulation.
	|

